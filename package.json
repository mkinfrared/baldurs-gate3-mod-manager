{
  "name": "example-app-korotkov",
  "version": "0.2.3",
  "description": "A foundation for scalable desktop apps",
  "keywords": [
    "Baldur's Gate 3",
    "Mod",
    "Manager"
  ],
  "homepage": "https://github.com/mkinfrared/baldurs-gate3-mod-manager",
  "bugs": {
    "url": "https://github.com/mkinfrared/baldurs-gate3-mod-manager/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mkinfrared/baldurs-gate3-mod-manager.git"
  },
  "license": "MIT",
  "author": {
    "name": "Mikhail Korotkov",
    "email": "mkinfrared@gmail.com"
  },
  "main": "./src/main/main.ts",
  "scripts": {
    "build": "concurrently \"pnpm run build:main\" \"pnpm run build:renderer\"",
    "build:dll": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.renderer.dev.dll.ts",
    "build:main": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.main.prod.ts",
    "build:renderer": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.renderer.prod.ts",
    "postinstall": "ts-node .erb/scripts/check-native-dep.js && electron-builder install-app-deps && pnpm run build:dll",
    "package": "ts-node ./.erb/scripts/clean.js dist && pnpm run build && electron-builder build -mw --publish never && pnpm run build:dll",
    "rebuild": "electron-rebuild --parallel --types prod,dev,optional --module-dir release/app",
    "start": "ts-node ./.erb/scripts/check-port-in-use.js && pnpm run start:renderer",
    "start:main": "cross-env NODE_ENV=development electronmon -r ts-node/register/transpile-only .",
    "start:preload": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.preload.dev.ts",
    "start:renderer": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack serve --config ./.erb/configs/webpack.config.renderer.dev.ts",
    "test": "jest",
    "prepare": "husky install",
    "lint": "pnpm format && pnpm lint:styles && pnpm lint:ts",
    "lint:ts": "eslint --cache --fix --ext .ts,.tsx .",
    "lint:styles": "stylelint \"**/*.scss\" --fix",
    "format": "prettier --write \".\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "coverage": "open-cli coverage/lcov-report/index.html",
    "release": "commit-and-tag-version"
  },
  "dependencies": {
    "@tanstack/react-query": "^4.35.7",
    "@tanstack/react-table": "^8.10.3",
    "@trpc/client": "^10.38.5",
    "@trpc/react-query": "^10.38.5",
    "@trpc/server": "^10.38.5",
    "comlink": "^4.4.1",
    "electron-debug": "^3.2.0",
    "electron-log": "^4.4.8",
    "electron-updater": "^6.1.4",
    "lodash-es": "^4.17.21",
    "node-stream-zip": "^1.15.0",
    "node-unrar-js": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0",
    "react-router-typesafe-routes": "^1.2.1",
    "react-use": "^17.4.0",
    "superjson": "^1.13.3",
    "yup": "^1.3.2",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.0",
    "@electron/notarize": "^2.1.0",
    "@electron/rebuild": "^3.3.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
    "@storybook/addon-essentials": "^7.4.5",
    "@storybook/addon-interactions": "^7.4.5",
    "@storybook/addon-links": "^7.4.5",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.4.5",
    "@storybook/react": "^7.4.5",
    "@storybook/react-webpack5": "^7.4.5",
    "@storybook/testing-library": "^0.2.2",
    "@svgr/webpack": "^8.1.0",
    "@teamsupercell/typings-for-css-modules-loader": "^2.5.2",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/jest": "^29.5.5",
    "@types/lodash-es": "4.17.9",
    "@types/node": "20.8.2",
    "@types/react": "^18.2.24",
    "@types/react-dom": "^18.2.8",
    "@types/react-test-renderer": "^18.0.3",
    "@types/webpack-bundle-analyzer": "^4.6.1",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "axios": "^1.5.1",
    "babel-plugin-transform-import-meta": "^2.2.1",
    "babel-preset-react-app": "^10.0.1",
    "browserslist-config-erb": "^0.0.3",
    "chalk": "^4.1.2",
    "commit-and-tag-version": "^11.2.4",
    "concurrently": "^8.2.1",
    "core-js": "^3.33.0",
